generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Inquiry {
  id        String   @id @default(cuid())
  service   String
  name      String
  email     String
  phone     String
  message   String
  status    String   @default("New")
  createdAt DateTime @default(now())
}



